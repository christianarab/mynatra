<h1>Menu_items</h1>
    <% @menu_items.each do |menu_item| %>
        <li><a href="/menu_items/<%= menu_item.id %>"><%= menu_item.id %></a></li>
        <ul>
        
            <li>name: <%= menu_item.name %></li>
        
            <li>description: <%= menu_item.description %></li>
        
            <li>price: <%= menu_item.price %></li>
        
            <li>meal_type: <%= menu_item.meal_type %></li>
        
        </ul>
    <% end %>
</ul>